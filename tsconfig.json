{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "./dist",
      "baseUrl": "../",
      "paths": {
        "@/*": [
          "xsigners_sdk/src/*"
        ],
        "@typed_contracts/*": [
          "typed_contracts/*"
        ]

      },
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  