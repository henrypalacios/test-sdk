{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "resolveJsonModule": true,
      "outDir": "./dist",
      "baseUrl": "./src",
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  